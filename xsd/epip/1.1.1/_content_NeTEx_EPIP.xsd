				<xsd:annotation>
					<xsd:documentation>Whether nearest crossing to ENTRANCE has dropped kerb.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attributeGroup ref="EntityAttributeGroup"/>
