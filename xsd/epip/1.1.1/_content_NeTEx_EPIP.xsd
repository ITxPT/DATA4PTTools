	<xsd:complexType name="PropertyOfDayStructure">
		<xsd:sequence>
			<xsd:element ref="Name" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Name of PROPERTY OF DAY</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="Description" minOccurs="0">
