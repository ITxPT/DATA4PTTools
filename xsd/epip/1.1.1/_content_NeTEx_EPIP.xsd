			<xsd:element ref="tariffZones" minOccurs="0"/>
			<xsd:element ref="stopAssignments" minOccurs="0"/>
			<xsd:element ref="journeyPatterns" minOccurs="0"/>
			<xsd:element ref="notices" minOccurs="0"/>
		</xsd:sequence>
