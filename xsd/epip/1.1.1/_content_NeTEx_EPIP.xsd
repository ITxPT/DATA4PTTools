			<xsd:annotation>
				<xsd:documentation>EPIP More types possible, but not explicitly added for now...</xsd:documentation>
			</xsd:annotation>
			<xsd:element ref="TypeOfFrame"/>
			<xsd:element ref="TypeOfPlace"/>
			<xsd:element ref="TypeOfFlexibleService"/>
