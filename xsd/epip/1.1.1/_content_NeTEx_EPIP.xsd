			<xsd:group ref="RestrictedSiteElementObjectElementGroup"/>
			<xsd:element ref="LevelRef" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Reference to level of SITE COMPONENT within SITE</xsd:documentation>
