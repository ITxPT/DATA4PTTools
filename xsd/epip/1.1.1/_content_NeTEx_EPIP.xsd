					<xsd:annotation>
						<xsd:documentation>Extra: Snow and Ice Submode. +v1.1s</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
		</xsd:sequence>
	</xsd:group>
