				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="firstClass">
				<xsd:annotation>
					<xsd:documentation>pti23_6</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="secondClass ">
				<xsd:annotation>
